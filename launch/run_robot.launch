<?xml version="1.0"?>
<launch>
  
     
    <!-- Node python scripts -->
    <node   name="kinematic_model" 
            pkg="pai" 
            output="screen" 
            type="kinematic_model.py" 
    />
    
    <!-- Right differential driver  -->
    <rosparam file="$(find pai)/config/driver_R_pinout.yaml" command="load" />
    <node   name="right_driver" 
            pkg="pai" output="screen"
            type="motor_driver.py" 
        >
        <remap from="/wheel_A_ctrl/command" to="/front_right_wheel_ctrl/command"/>
        <remap from="/wheel_B_ctrl/command" to="/back_right_wheel_ctrl/command"/>
    </node>

    
    <!-- Left differential driver  -->
    <rosparam file="$(find pai)/config/driver_L_pinout.yaml" command="load" />
    <node   name="left_driver" 
            pkg="pai" output="screen"
            type="motor_driver.py"     
     >   
        <remap from="/wheel_A_ctrl/command" to="/front_left_wheel_ctrl/command"/>
        <remap from="/wheel_B_ctrl/command" to="/back_left_wheel_ctrl/command"/>    
    </node>

    

</launch>
